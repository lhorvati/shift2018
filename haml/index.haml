                  zoom: 16,
                  center: shiftSpot
                });

                var contentString = "<strong>Shift Split Conference 2018</strong><br>Trg Gaje Bulata 1, Split<br>";

                var infowindow = new google.maps.InfoWindow({
                  content: contentString
                });

                var marker = new google.maps.Marker({
                  position: shiftSpot,
                  map: map,
                  title: 'Shift Conference 2018'
                });
                marker.addListener('click', function() {
                  infowindow.open(map, marker);
                });
                infowindow.open(map, marker);
              }
